{
    "builds": [
      {
        "src": "front-end/package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "build" }
      },
      {
        "src": "back-end/api/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/api/server.js" },
      { "src": "/(.*)", "dest": "/frontend/$1" }
    ],
    "env": {
        "CI": "false"
      }
  }
  